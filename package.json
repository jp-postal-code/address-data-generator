{
  "name": "@jp-postal-code/address-data-generator",
  "description": "Generate japan address data",
  "version": "0.1.2",
  "repository": "git@github.com:jp-postal-code/address-data-generator.git",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "commonjs",
  "bin": {
    "adg": "bin/adg.js"
  },
  "author": "shota <xyz.shotao@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "node -r @swc-node/register ./scripts/prebuild.ts",
    "build": "swc src/bin src/lib -d dist",
    "postbuild": "node -r @swc-node/register ./scripts/postbuild.ts",
    "type-check": "tsc",
    "test": "jest",
    "pree2e": "yarn build",
    "e2e": "jest -c jest-e2e.config.js",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@supercharge/promise-pool": "2.4.0",
    "commander": "10.0.0",
    "csv-parse": "5.3.6",
    "fast-glob": "3.2.12",
    "iconv-lite": "0.6.3",
    "jszip": "3.10.1",
    "listr2": "5.0.8",
    "progress": "2.0.3",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "@jest/globals": "29.5.0",
    "@swc-node/register": "1.6.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.45",
    "@swc/jest": "0.2.24",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@types/progress": "2.0.5",
    "@types/react": "18.0.33",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "eslint": "8.37.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "typescript": "5.0.3"
  }
}
